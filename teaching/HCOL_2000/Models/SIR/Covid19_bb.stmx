	<model>
		<variables>
			<stock name="Susceptible">
				<eqn>Total_population-Initial_Number_of_Infected</eqn>
				<outflow>becoming_infected</outflow>
				<non_negative/>
				<units>People</units>
			</stock>
			<stock name="Infected">
				<eqn>Initial_Number_of_Infected</eqn>
				<inflow>becoming_infected</inflow>
				<outflow>recovering</outflow>
				<outflow>dying</outflow>
				<non_negative/>
				<units>People</units>
			</stock>
			<stock name="Recovered">
				<eqn>0</eqn>
				<inflow>recovering</inflow>
				<non_negative/>
				<units>People</units>
			</stock>
			<stock name="Deceased">
				<eqn>0</eqn>
				<inflow>dying</inflow>
				<non_negative/>
				<units>People</units>
			</stock>
			<flow name="becoming\ninfected">
				<eqn>number_of_contacts_of_infected_with_susceptible*Probability_of_Transfer</eqn>
				<non_negative/>
				<units>People/Day</units>
			</flow>
			<flow name="recovering">
				<eqn>Infected/Time_to_Symptom_Free</eqn>
				<non_negative/>
				<units>People/Day</units>
			</flow>
			<flow name="dying">
				<eqn>Infected*Daily_Mortality_Rate</eqn>
				<non_negative/>
				<units>People/Day</units>
			</flow>
			<aux name="Total\npopulation">
				<eqn>7.8e9</eqn>
				<units>People</units>
			</aux>
			<aux name="Initial Number\nof Infected">
				<eqn>10</eqn>
				<units>People</units>
			</aux>
			<aux name="Probability\nof Transfer">
				<eqn>0.6</eqn>
				<format precision="0.01"/>
				<range min="0" max="1"/>
				<units>unitless</units>
			</aux>
			<aux name="Time to\nSymptom Free">
				<eqn>12</eqn>
				<units>Days</units>
			</aux>
			<aux name="Daily Mortality\nRate">
				<eqn>Crude_Mortality_Fraction_of_Infected/Days_Infected</eqn>
				<units>1/Day</units>
			</aux>
			<aux name="Crude Mortality Fraction\nof Infected">
				<eqn>0.034</eqn>
				<units>unitless</units>
			</aux>
			<aux name="Days Infected">
				<eqn>12</eqn>
				<units>Days</units>
			</aux>
			<aux name="Total Infected\nContacts per day">
				<eqn>Infected*Contacts_per_infected_per_day</eqn>
				<units>People/Day</units>
			</aux>
			<aux name="Contacts per infected\nper day">
				<eqn>20</eqn>
				<format precision="1"/>
				<range min="0" max="20"/>
				<units>People/People/Day</units>
			</aux>
			<aux name="number of contacts\nof infected with\nsusceptible">
				<eqn>(Susceptible/Total_population)*Total_Infected_Contacts_per_day</eqn>
				<units>People/Day</units>
			</aux>
			<aux name="Max Infected">
				<eqn>IF Infected &gt; PREVIOUS(SELF,-1) THEN Infected ELSE PREVIOUS(SELF,-1)</eqn>
				<units>People</units>
			</aux>
		</variables>
